	"version": "0.2.0",
	"configurations": [
		{
			"name": "TS Node DEV Current File",
			"type": "node",
			"request": "launch",
			"args": ["${fileBasename}"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${fileDirname}",
			"protocol": "inspector",
			"outputCapture": "std",
			"env": {
				"NODE_ENV": "development"
			}
		},
	]
}